<!DOCTYPE html>
<html>
  <head>
    <title>RailsSurvey</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <div class="box">

      <%= flash[:alert] %>
      <%= flash[:notice] %>

      <div id="left"></div>
      <div id="right"></div>
      
      <%= yield %>

      </div>
    </div>  
  </body>
</html>
